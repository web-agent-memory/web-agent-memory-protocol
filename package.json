{
	"name": "web-agent-memory-protocol",
	"version": "1.0.0",
	"private": true,
	"description": "Web Agent Memory Protocol (WAMP) monorepo - Browser API for contextual browsing data",
	"type": "module",
	"scripts": {
		"build": "pnpm -r build",
		"dev": "pnpm -r dev",
		"test": "pnpm -r test",
		"lint": "biome check --write .",
