		"monorepo"
	],
	"author": "Web Agent Memory Protocol Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/web-agent-memory/web-agent-memory-protocol.git"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@changesets/cli": "2.29.6",
		"jsdom": "26.1.0",
		"tslib": "2.8.1",
		"typescript": "5.9.2",
		"vite": "7.1.3",
		"vite-plugin-dts": "4.5.4",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": ">=14.0.0",
		"pnpm": ">=8.0.0"
	}
