{
	"name": "@wamp/extension-sdk",
	"version": "1.0.0",
	"description": "SDK for building Web Agent Memory Protocol (WAMP) browser extensions and providers",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"lint": "biome check --write .",
		"format": "biome format --write .",
		"check": "pnpm typecheck && pnpm lint && pnpm test",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"web-agent-memory-protocol",
		"wamp",
		"memory-protocol",
		"browser-extension",
		"chrome-extension",
		"firefox-extension",
		"extension-sdk",
		"provider-sdk",
		"context",
		"ai",
		"personalization",
		"web-agent"
	],
	"author": "Web Agent Memory Protocol Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/web-agent-memory/web-agent-memory-protocol.git",
		"directory": "extension-sdk"
	},
	"dependencies": {
		"@wamp/protocol": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@types/chrome": "0.1.4",
		"@types/jsdom": "21.1.7",
		"@types/node": "24.3.0",
		"jsdom": "26.1.0",
		"tslib": "2.8.1",
		"typescript": "5.9.2",
		"vite": "7.1.3",
		"vite-plugin-dts": "4.5.4",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
